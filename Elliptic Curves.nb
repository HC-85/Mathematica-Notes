Cell[TextData[{
 "Given two points: ",
 Cell[BoxData[
  FormBox[
   RowBox[{"P", "=", 
    RowBox[{"(", 
     RowBox[{
      SubscriptBox["x", "1"], ",", 
      SubscriptBox["y", "1"]}], ")"}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "eb7996d9-74f9-473f-acd3-de9600e77bb7"],
 ", ",
 Cell[BoxData[
  FormBox[
   RowBox[{"Q", "=", 
    RowBox[{"(", 
     RowBox[{
      SubscriptBox["x", "2"], ",", 
      SubscriptBox["y", "2"]}], ")"}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "0882091b-188a-4a46-b53d-840d5d1f83ab"],
 "\nWe need to find the points that intersect the line ",
 Cell[BoxData[
  FormBox[
   RowBox[{"y", "=", 
    RowBox[{
     RowBox[{"s", " ", "x"}], "+", "m"}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "e0a398b7-b76a-44a3-8355-5194651d502d"],
 "\n\[CenterDot]We square and equate:\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox[
     RowBox[{"(", 
      RowBox[{
       RowBox[{"s", " ", "x"}], " ", "+", " ", "m"}], ")"}], "2"], "=", 
    RowBox[{
     RowBox[{
      RowBox[{
       SuperscriptBox["s", "2"], 
       SuperscriptBox["x", "2"]}], "+", 
      RowBox[{"2", " ", "s", " ", "x", " ", "m"}], " ", "+", 
      SuperscriptBox["m", "2"]}], "=", 
     RowBox[{
      SuperscriptBox["x", "3"], "+", 
      RowBox[{"a", " ", "x"}], " ", "+", "b"}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "14400847-1cf2-4ad1-b5a9-50d32ddd1d42"],
 "\n\[CenterDot]The slope ",
 Cell[BoxData[
  FormBox["s", TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "6d44e01c-f38c-49e5-9446-1a224d4d4569"],
 " is: \n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{"s", "=", 
    FractionBox[
     RowBox[{
      SubscriptBox["y", "2"], "-", 
      SubscriptBox["y", "1"]}], 
     RowBox[{
      SubscriptBox["x", "2"], "-", 
      SubscriptBox["x", "1"]}]]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "4bbb2adb-acae-4453-9b63-b5ad617d3d91"],
 "\n\[CenterDot]The y-intercept:\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{"m", "=", 
    RowBox[{
     SubscriptBox["y", "1"], "-", 
     RowBox[{
      FractionBox[
       RowBox[{
        SubscriptBox["y", "2"], "-", 
        SubscriptBox["y", "1"]}], 
       RowBox[{
        SubscriptBox["x", "2"], "-", 
        SubscriptBox["x", "1"]}]], 
      SubscriptBox["x", "1"]}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "a629a5c9-4e8d-4aa8-9b04-1fd7457f4c64"],
 "\n\[CenterDot]Substituting: \n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      SuperscriptBox[
       RowBox[{"(", 
        FractionBox[
         RowBox[{
          SubscriptBox["y", "2"], "-", 
          SubscriptBox["y", "1"]}], 
         RowBox[{
          SubscriptBox["x", "2"], "-", 
          SubscriptBox["x", "1"]}]], ")"}], "2"], 
      SuperscriptBox["x", "2"]}], "+", 
     RowBox[{"2", " ", 
      RowBox[{"x", "(", 
       FractionBox[
        RowBox[{
         SubscriptBox["y", "2"], "-", 
         SubscriptBox["y", "1"]}], 
        RowBox[{
         SubscriptBox["x", "2"], "-", 
         SubscriptBox["x", "1"]}]], ")"}], " ", 
      RowBox[{"(", 
       RowBox[{
        SubscriptBox["y", "1"], "-", 
        RowBox[{
         FractionBox[
          RowBox[{
           SubscriptBox["y", "2"], "-", 
           SubscriptBox["y", "1"]}], 
          RowBox[{
           SubscriptBox["x", "2"], "-", 
           SubscriptBox["x", "1"]}]], 
         SubscriptBox["x", "1"]}]}], ")"}]}], " ", "+", 
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{
        SubscriptBox["y", "1"], "-", 
        RowBox[{
         FractionBox[
          RowBox[{
           SubscriptBox["y", "2"], "-", 
           SubscriptBox["y", "1"]}], 
          RowBox[{
           SubscriptBox["x", "2"], "-", 
           SubscriptBox["x", "1"]}]], 
         SubscriptBox["x", "1"]}]}], ")"}], "2"]}], "=", 
    RowBox[{
     SuperscriptBox["x", "3"], "+", 
     RowBox[{"a", " ", "x"}], " ", "+", "b"}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "47593bec-3789-4a62-8240-0da1419feb54"],
 "\n\[CenterDot]Rearranging:\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{" ", 
    RowBox[{
     RowBox[{
      SuperscriptBox["x", "3"], "+", 
      RowBox[{
       RowBox[{"[", 
        RowBox[{"-", 
         SuperscriptBox[
          RowBox[{"(", 
           FractionBox[
            RowBox[{
             SubscriptBox["y", "2"], "-", 
             SubscriptBox["y", "1"]}], 
            RowBox[{
             SubscriptBox["x", "2"], "-", 
             SubscriptBox["x", "1"]}]], ")"}], "2"]}], "]"}], 
       SuperscriptBox["x", "2"]}], "+", 
      RowBox[{
       RowBox[{"[", 
        RowBox[{"a", "-", 
         RowBox[{"2", " ", 
          RowBox[{"(", 
           FractionBox[
            RowBox[{
             SubscriptBox["y", "2"], "-", 
             SubscriptBox["y", "1"]}], 
            RowBox[{
             SubscriptBox["x", "2"], "-", 
             SubscriptBox["x", "1"]}]], ")"}], " ", 
          RowBox[{"(", 
           RowBox[{
            SubscriptBox["y", "1"], "-", 
            RowBox[{
             FractionBox[
              RowBox[{
               SubscriptBox["y", "2"], "-", 
               SubscriptBox["y", "1"]}], 
              RowBox[{
               SubscriptBox["x", "2"], "-", 
               SubscriptBox["x", "1"]}]], 
             SubscriptBox["x", "1"]}]}], ")"}]}]}], "]"}], "x"}], "+", 
      RowBox[{"[", 
       RowBox[{"b", "-", 
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{
           SubscriptBox["y", "1"], "-", 
           RowBox[{
            FractionBox[
             RowBox[{
              SubscriptBox["y", "2"], "-", 
              SubscriptBox["y", "1"]}], 
             RowBox[{
              SubscriptBox["x", "2"], "-", 
              SubscriptBox["x", "1"]}]], 
            SubscriptBox["x", "1"]}]}], ")"}], "2"]}], "]"}]}], "=", "0"}]}], 
   TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "a534ad2d-73ff-426d-a0b6-f9491f14e30a"],
 " \n\[CenterDot]We already know two solutions: "
